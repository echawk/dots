
shift + Up
	volctrl i

shift + Down
	volctrl d

ctrl + shift + Up
	volctrl i 1

ctrl + shift + Down
	volctrl d 1

XF86AudioRaiseVolume
	volctrl i

XF86AudioLowerVolume
	volctrl d

XF86AudioMute
	volctrl m

XF86MonBrightnessDown
	bri d

XF86MonBrightnessUp
	bri i

super + Return
	terminal

# Start Applications
alt + d
	launcher

super + e
	em

super + shift + e
	emacs

alt + ctrl + x
	xkill

super + z
	lockscreen

super + shift + z
	dmenuexit.sh

#MISC
super + b
	$BROWSER

Print
	maim pic-full-"$(date '+%y%m%d-%H%M-%S').png"

shift + Print
	maimpick

super + m
	emacs -f mu4e

alt + semicolon
	expose

alt + p
	shopen "$(xclip -o)"

